sudo apt install git -y
git --version
git config --list


git commit -m "first"
git tag 0.1.0	VS	git tag --delete 0.1.0

git log
git log --pretty=oneline

# show commits on branchA thar are not on branchB
	git log branchB..branchA --pretty=oneline
	git log --pretty=oneline branchB..branchA	
	git log --pretty=oneline master..	# show all commits but not in master

git log --follow <file-name>	# list version history for a file, including renames

git branch <new-branch-name>
git branch -d <branch-name>		OR	 git branch --delete <branch-name>
git branch #To Check current branch & lists of branch

"Renaming Git Branch"
git checkout <old-branch-name>
git branch -m <new-branch-name>
git push origin <new-branch-name>		# push renamed branch to github
git push origin --delete <old-branch-name>		# del old branch on github

git merge master 	# merge commit messages of master branch to a current working branch
git merge <branch-name>	# merge commit messages of <branch-name> to master branch
git push origin master	# push current git branch code to git-hub master branch
git push origin <branch-name>	(or) 	git push --force origin <branch-name>
git pull origin master (or) git pull origin <branch-name>

git checkout <commit-id>
git checkout <branch-name>
git checkout master
git checkout 0.1.0

